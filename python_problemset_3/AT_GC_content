#!/usr/bin/env python3
dna = ''
A_count = dna.count('A')
T_count = dna.count('T')
G_count = dna.count('G')
C_count = dna.count('C')
total_nuc = len(dna)
print(f'The AT content in this sequence is {(A_count+T_count)/total_nuc}')
print(f'The GC content in this sequence is {(G_count+C_count)/total_nuc}')
